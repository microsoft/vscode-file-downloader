{
    "name": "file-downloader",
    "displayName": "File Downloader",
    "description": "Exposes an API that allows other extensions to download files.",
    "publisher": "mindaro-dev",
    "license": "MIT",
    "version": "1.0.13",
    "repository": {
        "type": "git",
        "url": "https://github.com/microsoft/vscode-file-downloader.git"
    },
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Other"
    ],
    "capabilities": {
        "virtualWorkspaces": {
            "supported": true,
            "description": "This extension does not write to the workspace location."
        },
        "untrustedWorkspaces": {
            "supported": true,
            "description": "This extension does not write to the workspace location."
        }
    },
    "activationEvents": [],
    "main": "./dist/extension",
    "contributes": {
        "commands": []
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "compile": "npm run webpack",
        "lint": "eslint src --ext ts --fix",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "test": "npm run test-compile && node ./out/test/runTest.js",
        "test-compile": "npm run compile && tsc -p ./"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.23",
        "@types/rimraf": "^3.0.2",
        "@types/unzipper": "^0.10.5",
        "@types/uuid": "^8.3.4",
        "@types/vscode": "^1.66.0",
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.17.0",
        "@vscode/test-electron": "2.3.8",
        "eslint": "^8.12.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsdoc": "^38.1.6",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-tslint": "^3.1.0",
        "glob": "^7.2.0",
        "license-checker-webpack-plugin": "^0.2.1",
        "mocha": "^9.2.2",
        "ts-loader": "^9.2.8",
        "typescript": "^4.6.3",
        "webpack": "^5.71.0",
        "webpack-cli": "^4.9.2"
    },
    "dependencies": {
        "extract-zip": "^2.0.1",
        "axios": "^0.26.1",
        "rimraf": "^3.0.2",
        "uuid": "^8.3.2"
    }
}